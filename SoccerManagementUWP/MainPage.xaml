<Page
    x:Class="SoccerManagementUWP.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:SoccerManagementUWP"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Loaded="Page_Loaded">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <StackPanel>
            <Button x:Name="b_showPlayerList" Content="PlayerList" HorizontalAlignment="Left" Margin="5" VerticalAlignment="Top" Click="b_showPlayerList_Click" Visibility="Collapsed"></Button>
            <Button x:Name="b_showPlayerProfile" Content="PlayerProfile" HorizontalAlignment="Left" Margin="5" VerticalAlignment="Top" Click="b_showPlayerProfile_Click" Visibility="Collapsed"></Button>
            <Button x:Name="btn_testRunCommand" Content="Test Run Commands" HorizontalAlignment="Left" Margin="5" Click="btn_testRunCommand_Click"></Button>
            <Button x:Name="btn_generator" Content="Data Generator" HorizontalAlignment="Left" Margin="5" Click="btn_generator_Click"></Button>
            <Button x:Name="btn_queryF" Content="Queries" HorizontalAlignment="Left" Margin="5" Click="btn_query_F_Click"></Button>
        </StackPanel>
    </Grid>
</Page>
